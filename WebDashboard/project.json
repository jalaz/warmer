{
  "userSecretsId": "Warmer.WebDashboard",

  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.1.0",
      "type": "platform"
    },
    "Microsoft.AspNetCore.Server.IISIntegration": "1.2.0-preview1-23225",
    "Microsoft.AspNetCore.Routing": "1.2.0-preview1-23225",
    "Microsoft.AspNetCore.Server.Kestrel": "1.2.0-preview1-23225",
    "Microsoft.Extensions.Configuration.Json": "1.2.0-preview1-23225",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.2.0-preview1-23225",
    "Microsoft.Extensions.Logging": "1.2.0-preview1-23225",
    "Microsoft.Extensions.Logging.Console": "1.2.0-preview1-23225",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.2.0-preview1-23225"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6"
      ]
    }
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "appsettings.json",
      "web.config"
    ]
  },

  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
